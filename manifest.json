{
    "manifest_version": 3,
    "name": "Form Input Name Viewer",
    "version": "1.1",
    "description": "Displays the name or data-testid attribute of form inputs",
    "permissions": ["scripting", "activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  